<main>
  <div class="container">
    <div class="row">
      <h4>ジャンル編集</h4>

      <span>ジャンル名</span>

      <%= form_with model: @address, url: admin__update_path, method: :patch do |f| %>
        <%= f.text_field :name %>
　　　　<%= f.submit '変更を保存', class: "btn btn-success" %>
      <% end %>

    </div>
  </div>
</main>
